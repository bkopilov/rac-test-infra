apiVersion: k8s.cni.cncf.io/v1
kind: NetworkAttachmentDefinition
metadata:
  name: {{bridge_name}}
  namespace: default
spec:
  config: |-
    {
        "cniVersion": "0.3.1",
        "name": "{{bridge_name}}",
        "type": "bridge",
        "bridge": "{{bridge_name}}",
        "ipam": {},
        "macspoofchk": true,
        "preserveDefaultVlan": false
    }
